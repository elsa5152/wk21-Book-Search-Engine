    "@apollo/react-hooks": "^4.0.0",
    "apollo-boost": "^0.4.9",
    "apollo-server-express": "^3.10.0",
    "if-env": "^1.0.4",
    "mongoose": "^6.5.1",
    "proxy": "^1.0.2",
    "redis-server": "^1.2.2",
    "schema-utils": "^4.0.0"
  }
